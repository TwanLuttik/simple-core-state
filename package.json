	"version": "1.0.0-beta.1",
	"main": "dist/index.js",
	"types": "dist/index.d.ts",
	"license": "MIT",
	"private": false,
	"homepage": "https://github.com/TwanLuttik/simple-core-state#readme",
	"repository": {
